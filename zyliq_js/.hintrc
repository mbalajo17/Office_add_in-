{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off",
        "frame-title": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "summary",
          "details",
          "script[type=module]"
        ]
      }
    ],
    "meta-viewport": "off"
  }
}